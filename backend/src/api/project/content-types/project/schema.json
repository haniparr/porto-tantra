{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "Project",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "client": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "client",
      "required": true
    },
    "year": {
      "type": "string",
      "required": true
    },
    "services": {
      "type": "string",
      "required": true
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "sections": {
      "type": "component",
      "repeatable": true,
      "component": "project.project-section"
    },
    "featured": {
      "type": "boolean",
      "default": false
    },
    "testimonial": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::testimonial.testimonial"
    },
    "credits": {
      "type": "component",
      "repeatable": true,
      "component": "project.project-credit"
    },
    "dummy": {
      "type": "string"
    }
  }
}